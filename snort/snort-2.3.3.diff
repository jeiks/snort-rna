+FlintType ACT_2_DERIV_Logistic (struct Unit *unit_ptr)
+{
+
+    return (GET_UNIT_ACT (unit_ptr) * (1.0 - GET_UNIT_ACT (unit_ptr)) 
+	    * (2 * GET_UNIT_ACT (unit_ptr) - 1.0));
+
+}
