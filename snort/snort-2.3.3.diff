+/*^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^*/
+void  kra2_checkReset (void)
+{
+   FlintType NormR;
+
+   NormR = ART2_PARAM_e + kra2_L2_Norm (ART2_R_LAY);
+
+   if ((kra2_f1_stable()) && (kra2_topdn_phase()) && (kra2_get_rho() / NormR > 1)) {
+      GlobalReset  = TRUE;
+   } else {
+      GlobalReset  = FALSE;
+   } /*if*/
+
+} /* kra2_checkReset () */
