+/*^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^*/
+bool  kram_AllMapUnitsActive (void)
+{
+    TopoPtrArray   topo_ptr = ArtMap_map_layer;
+
+    if (topo_ptr == NULL) {
+       return (FALSE);
+    } /*if*/
+
+    while (*topo_ptr != NULL) {
+       if ((*topo_ptr)->act < 0.9) {
+          return (FALSE);
+       } /*if*/
+    } /*while*/
+
+    return (TRUE);
+
+} /* kram_AllMapUnitsActive () */
