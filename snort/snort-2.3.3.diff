+			 float i_smooth, int init_type)
+{
+    register struct Unit    *unit_ptr;
+    register struct Unit    *h_unit_ptr;
+    register struct Link    *link_ptr;
+    register Patterns       current_out_pattern;
+    register TopoPtrArray   topo_ptr;
