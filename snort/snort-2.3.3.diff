+#define PROPAGATE_THROUGH_INPUT_LAYER(inputUnitPtr,dummy,pattern) \
+    FOR_ALL_INPUT_UNITS(inputUnitPtr,dummy){ \
+        CALCULATE_INPUTUNIT_ACTIVATION_AND_OUTPUT(inputUnitPtr,*pattern++); \
+    }
