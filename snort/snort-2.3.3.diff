+      TOPO_MSG_NO_OF_UNITS_IN_LAYER ("ARTb comparison");
+   } /*if*/
+
+   *topo_ptr++ = NULL;
+
+
+
+   /* determine recognition units of ARTb
+   */
+   ret_code = kram_get_RecUnits_b (&topo_ptr, &ArtMap_NoOfRecUnits_b);
+
+   if (ret_code != KRERR_NO_ERROR) {
+      return (ret_code);
