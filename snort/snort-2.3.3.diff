+void    kr_updateUnitOutputs(void)
+{
+  register struct Unit   *unit_ptr;
+
+
+  FOR_ALL_UNITS( unit_ptr )
+    if ( (unit_ptr->flags & UFLAG_INITIALIZED) == UFLAG_INITIALIZED)
+      {     /*  unit is in use and enabled  */
