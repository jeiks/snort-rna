+/*****************************************************************************
+  FUNCTION : getTType 
+
+  PURPOSE  : 
+  NOTES    : 
+
+  RETURNS  : 
+  UPDATE   : 
+******************************************************************************/
+
+static char  *getTType(int st)
+{
+  static char   *ttype[] = { "-", "i", "o", "d", "h", "s" ,
+                             "si", "so", "sh", "sd"}; 
+
+  switch ( st )  {
+    case  INPUT:
+      return( ttype[1] );
+    case  OUTPUT:
+      return( ttype[2] );
+    case  DUAL:
+      return( ttype[3] );
+    case  HIDDEN:
+      return( ttype[4] );
+    case  SPECIAL:
+      return( ttype[5] );
+    case  SPECIAL_I:      
+      return( ttype[6] ); 
+    case  SPECIAL_O:      
+      return( ttype[7] ); 
+    case  SPECIAL_H:      
+      return( ttype[8] ); 
+    case  SPECIAL_D:      
+      return( ttype[9] ); 
+    default:
+      return( ttype[0] );
+  }
+}
