+   } /*if*/
+
+   *topo_ptr++ = NULL;
+
+
+   /* determine local reset units of ARTb
+   */
+   ret_code = kram_get_RstUnits_b (&topo_ptr, &no_of_rstb_units);
+
+   if (ret_code != KRERR_NO_ERROR) {
+      return (ret_code);
