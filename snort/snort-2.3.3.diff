+}
+*/
+
+/*****************************************************************************
+  FUNCTION : krm_releaseAllSites
+
+  PURPOSE  : release the unit-site and all following sites (at this unit)
+  NOTES    :
+
+  RETURNS  :
+  UPDATE   : 
+******************************************************************************/
+void    krm_releaseAllSites(struct Site *first_site_ptr)
+{
+  struct Site  *curr,
+               *next,
+               *free;
+
+
+  free = free_site_ptr;
+  curr = first_site_ptr;
+
+  while (curr != NULL)  {
+    --NoOfSites;
+    --NoOfNetSites;
+
+    next = curr->next;
+    curr->next = free;
+
+    free = curr;
+    curr = next;
+  }
+
+  free_site_ptr = free;
+}
+
