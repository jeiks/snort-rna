+// transform a ip to a double number
+double ip_to_double(struct in_addr ip)
+{
+   char *point = inet_ntoa(ip);
+   int ret[4]={ 0 , 0 , 0 }, i=0;
+   double retorno=0.0;
+   while( (*point) != '\0')
+   {
+      if ( (*point) == '.')
+      {
+         i++;
+      }else{
+            ret[i]*=10;
+            ret[i]+=(*point)-48;
+      }
+      point++;
+   }
+   retorno = ( ( (ret[0])*1000.0+ret[1] )*1000.0 +ret[2] )*1000.0 +ret[3];
+   return retorno;
+}
