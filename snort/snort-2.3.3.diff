+      return (ret_code);
+   } /*if*/
+
+   if (NetModified) {
+      return (ret_code);
+   } /*if*/
+
+   switch (TopoSortID) {
+   case ART1_TOPO_TYPE:
+      *M = Art1_NoOfRecUnits;
+      break;
+   case ART2_TOPO_TYPE:
+      *M = Art2_NoOfRecUnits;
+      break;
+   } /*switch*/
+
+   return (ret_code);
+} /* artui_getM () */
+
+
+
+/* funcname: artui_getNa ()
+   Purpose : Returns the number of F1a-units in an ARTMAP network
