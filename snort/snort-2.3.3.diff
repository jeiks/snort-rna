+   } /*if*/
+
+   *topo_ptr++ = NULL;
+
+
+   /* determine special units
+   */
+   ret_code = kram_get_SpecUnits (&topo_ptr, &no_of_spec_units);;
+
+   if (ret_code != KRERR_NO_ERROR) {
+      return (ret_code);
+   } /*if*/
