+/*****************************************************************************
+  FUNCTION : my_strstr 
+
+  PURPOSE  : Find the first occurrence of find in s.
+             (slow version)
+  NOTES    : 
+
+  RETURNS  : 
+  UPDATE   : 
+******************************************************************************/
+static char  *my_strstr(char *s, char *find)
+{
+  register char c, sc;
+  register size_t len;
+
+  if ((c = *find++) != 0)  {
+    len = strlen(find);
+    do  {
+      do  {
+	if ((sc = *s++) == 0)	return (NULL);
+      }
+      while (sc != c);
+    }
+    while (strncmp(s, find, len) != 0);
+    s--;
+  }
+
+  return ((char *) s);
+}
