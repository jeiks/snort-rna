+static krui_err  krio_writeConnectionDefs(void)
+{
+  bool  second;
+  int   target_unit, unit_no,
+        err;
+
+
+  FlintType  weight;
+
+
+  if (NoOfLinks == 0)  return( 0 );
+
+  err = krio_fmtShapeing( CONNECT_DEF );
+  retchk( err );
