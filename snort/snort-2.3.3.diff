+/*^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^*/
+krui_err kram_init_i_act (double rhoa, double rhob, double rho)
+{
+   int                    ret_code = KRERR_NO_ERROR;
+
+   register struct Unit   *unit_ptr;
+
+
+   FOR_ALL_UNITS (unit_ptr) {
+
+      switch (unit_ptr->lln) {
+
+      case ARTMAP_SPECa_LAY:
+         switch (unit_ptr->lun) {
+         case ARTMAP_G1a_UNIT :
+            unit_ptr->i_act = 0.0;
+            break;
+         case ARTMAP_RIa_UNIT :
+            unit_ptr->i_act = 1.0;
+            break;
+         case ARTMAP_RGa_UNIT :
+            unit_ptr->i_act = 1.0;
+            break;
+         case ARTMAP_RHOa_UNIT :
+            unit_ptr->i_act = (FlintType) rhoa;
+            break;
+         default :
+            unit_ptr->i_act = 0.0;
+            break;
+         } /*switch*/
+         break;
+
+      case ARTMAP_SPECb_LAY:
+         switch (unit_ptr->lun) {
+         case ARTMAP_G1b_UNIT :
+            unit_ptr->i_act = 0.0;
+            break;
+         case ARTMAP_RIb_UNIT :
+            unit_ptr->i_act = 1.0;
+            break;
+         case ARTMAP_RGb_UNIT :
+            unit_ptr->i_act = 1.0;
+            break;
+         case ARTMAP_RHOb_UNIT :
+            unit_ptr->i_act = (FlintType) rhob;
+            break;
+         default :
+            unit_ptr->i_act = 0.0;
+            break;
+         } /*switch*/
+         break;
+
+      case ARTMAP_SPEC_LAY:
+         switch (unit_ptr->lun) {
+         case ARTMAP_G_UNIT:
+            unit_ptr->i_act = 1.0;
+            break;
+         case ARTMAP_RHO_UNIT:
+            unit_ptr->i_act = (FlintType) rho;
+            break;
+         default:
+            unit_ptr->i_act = 0.0;
+            break;
+         } /*switch*/
+         break;
+
+      default :
+         unit_ptr->i_act = 0.0;
+         break;
+      } /*switch*/
+
+   } /*FOR_ALL_UNITS*/
+
+
+   return (ret_code);
+
+} /* kram_init_i_act () */
