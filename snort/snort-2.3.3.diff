+/*  Elliott Function
+*/
+FlintType   ACT_Elliott(struct Unit *unit_ptr)
+{
+  ACT_FUNC_DEFS
+  register FlintType  sum;
+
+
+  sum =  0.0;
+  if (GET_FIRST_UNIT_LINK( unit_ptr ))
+    do
+      sum += GET_WEIGHTED_OUTPUT;
+    while (GET_NEXT_LINK);
+  else
+    if (GET_FIRST_SITE( unit_ptr ))
+      do
+	sum += GET_SITE_VALUE;
+      while (GET_NEXT_SITE);
+
+  sum += GET_UNIT_BIAS(unit_ptr);
+  if (sum <= 0.0)
+      return (FlintType) sum/(1.0 - sum);
+  else
+      return (FlintType) sum/(1.0 + sum);
