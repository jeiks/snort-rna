+
+   *topo_ptr++ = NULL;
+
+
+   /* determine r units
+   */
+   topo_layer[7] = topo_ptr;
+   ret_code = kra2_get_RUnits (&topo_ptr, &no_of_r_units);
+
+   if (ret_code != KRERR_NO_ERROR) {
