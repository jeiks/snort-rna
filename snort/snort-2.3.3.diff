+  UPDATE   :
+******************************************************************************/
+bool  krui_isFunction(char *func_name, int func_type)
+{
+    FunctionPtr  dummy_func_ptr;
+    bool        is_func;
+
+    is_func = krf_funcSearch( func_name, func_type, &dummy_func_ptr );
+    KernelErrorCode = KRERR_NO_ERROR;
+    return( is_func );
+}
