+
+#ifdef DEBUG_PATTERN
+static print_page_info(struct patternpage *page)
+{
+    int fl;
+    int cnt;
+
+    if (!page)
+	printf("empty page\n");
+    else
+    {
+	printf("id: %0lx, size: %d, slots: %d, free_slots: %d, list: ",
+	       (long) page, page->slotsize, page->no_of_slots, page->no_of_free_slots);
+	fl = page->firstfree;
+	cnt = 0;
+	do
+	{
+	    printf("%d ", fl);
+	    fl = *((int *) ((float *) page->data + fl));
+	    cnt++;
+	} while (fl != -1 && cnt < 20);
+	if (fl != -1)
+	    printf("...");
+	printf("\n");
+    }
+}
