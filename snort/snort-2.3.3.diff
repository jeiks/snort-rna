+
+/*  Sigmoid Derivation Function
+*/
+FlintType   ACT_DERIV_Logistic(struct Unit *unit_ptr)
+{
+    return( GET_UNIT_ACT( unit_ptr ) * (1.0 - GET_UNIT_ACT( unit_ptr )) );
+}
