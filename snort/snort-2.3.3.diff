+            sum += GET_SITE_VALUE;
+         } while (GET_NEXT_SITE);
+      } /*if*/
+   } /*if*/
+
+   if ((sum > 0.8) && (sum < 1.2)) {
+      return (1.0);
+   } else {
+      return (0.0);
+   } /*if*/
+
+} /* ACT_exactly_1 */
