+
+
+         if (!got_site1 || !got_site2) {
+            TOPO_MSG_SITE_MISSING (unit_ptr);
+         } /*if*/
+
+
+      } else {
+
+         TOPO_MSG_UNEXPECTED_DIRECT_INPUTS (unit_ptr);
+
+      } /*if*/
+
