+        break;
+
+      case  LAYER_DEF:
+        len = Max( layer_no_len, strlen( formats[ 16 ] ) );
+	len1 = len + 1;
+        sprintf( aux, "%d", len );
+	mstrcpy( fmt_shape1, "\n%", aux, "d |", (char *) 0 );
