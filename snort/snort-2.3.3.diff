+#ifdef HAVE_VALUES_H
+#include <values.h>
+#endif
+#include <string.h>
+
+#include "kr_typ.h"	    /*	Kernel types and constants  */
+#include "kr_const.h"       /*  Constant Declarators for SNNS-Kernel  */
+#include "func_mac.h"	    /*	Transfer function macros  */
+#include "glob_typ.h"
