+/*****************************************************************************
+  FUNCTION : krio_repchar 
+
+  PURPOSE  : 
+  NOTES    : 
+
+  RETURNS  : 
+  UPDATE   : 
+****************************************************************************/
+static char  *krio_repchar(char c, int N)
+{
+  int   i;
+  static char  str[180];
+
+  if ( (N - 1) >= sizeof (str))   N = sizeof (str) - 1;
+
+  for (i = 0; i < N; i++)
+    str[ i ] = c;
+
+  str[N] = 0;
+  return( str );
+}
