+
+
+   FOR_ALL_UNITS (unit_ptr) {
+
+      if (IS_SPECIAL_UNIT(unit_ptr)) {
+         count++;
+      } /*if*/
+
+   } /*FOR_ALL_UNITS*/
+
+   return (count);
+
+
