+/*  Linear Activation Function
+*/
+FlintType   ACT_Linear(struct Unit *unit_ptr)
+{
+  ACT_FUNC_DEFS
+  register FlintType  sum;
+
+
+  sum =  0.0;
+  if (GET_FIRST_UNIT_LINK( unit_ptr ))
+    do
+      sum += GET_WEIGHTED_OUTPUT;
+    while (GET_NEXT_LINK);
+  else
+    if (GET_FIRST_SITE( unit_ptr ))
+      do
+	sum += GET_SITE_VALUE;
+      while (GET_NEXT_SITE);
+
+  return( sum );
+}
+
+/*  Brain-State-in-a-Box Function
