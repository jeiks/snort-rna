+#define CALCULATE_INPUTUNIT_ACTIVATION_AND_OUTPUT(UnitPtr,value) \
+    UnitPtr->Out.output = ((UnitPtr->out_func == OUT_IDENTITY) ? \
+                          (UnitPtr->act = value) : \
+                          (*UnitPtr->out_func) (UnitPtr->act = value)); \
