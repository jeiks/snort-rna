+    if ( error_code >= NoOfMessages )  error_code = 0; /* invalid error code */
+
+    switch (-error_code)
+        {
+          case KRERR_CYCLES:
+          case KRERR_DEAD_UNITS:
+          case KRERR_I_UNITS_CONNECT:
+          case KRERR_O_UNITS_CONNECT:
