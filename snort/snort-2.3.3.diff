+/*^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^*/
+static FlintType kra2_compute_l2_norm (int Layer)
+{
+   register TopoPtrArray    topo_ptr = topo_layer[Layer-1];
+   register FlintType       sum = 0.0;
+   register FlintType       val;
+
+   while (*topo_ptr != NULL) {
+      val = (*topo_ptr)->Out.output;
+      sum += val * val;
+      topo_ptr++;
+   } /*while*/
+   return ( sqrt((double) sum) );
+}
